<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd"
         updateCheck="true"
         monitoring="autodetect"
         dynamicConfig="true">
    <defaultCache eternal="true" maxElementsInMemory="200" overflowToDisk="false" />
    <cache name="forecastCache" eternal="false" maxElementsInMemory="10000" overflowToDisk="false" timeToLiveSeconds="500"/>
</ehcache>